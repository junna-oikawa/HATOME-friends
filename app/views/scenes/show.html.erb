
<div class="main-content">
  <div class="show-main">
    <%= link_to scene_path(@pre_scene.id), class:"round-btn show-btn pre-btn" do %>
      <%= image_tag 'arrow.png', :width => '90' , style:"transform: rotate(-90deg);"%>
    <% end %>
    <div id="scene-container">
      <div class="scene">
      </div>
    </div>
    <%= link_to scene_path(@next_scene.id), class:"round-btn show-btn next-btn" do %>
      <%= image_tag 'arrow.png', :width => '90' , style:"transform: rotate(90deg);"%>
    <% end %>
  </div>
  <div id="visibility" class="round-btn xform-btn sc-show-elt-btn">
    <%= image_tag 'btn11.png', :width => '90' %>
  </div>
<div>



<div id="hidden">
    <input type="hidden" id="data" value="<%= @scene.json_data %>">
  </div>
<script src="https://unpkg.com/konva@8.2.2/konva.min.js"></script>
<%= stylesheet_link_tag 'scenes' %>
